{
  "name": "faed/hyperf-saas-helper",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf"
  ],
  "description": "",
  "autoload": {
    "psr-4": {
      "Faed\\HyperfSaasHelper\\": "src/"
    },
    "files": [
      "src/Helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "tests"
    }
  },
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.10",
    "hyperf/command": "^3.1",
    "hyperf/config": "^3.1",
    "hyperf/constants": "^3.1",
    "hyperf/di": "3.1.*",
    "hyperf/framework": "^3.1",
    "hyperf/http-server": "^3.1",
    "hyperf/support": "^3.1",
    "hyperf/swagger": "^3.1",
    "nesbot/carbon": "^2.21"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "^10.0",
    "swoole/ide-helper": "dev-master"
  },
  "suggest": {
    "swow/swow": "Required to create swow components."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "hyperf": {
      "config": "Faed\\HyperfSaasHelper\\ConfigProvider"
    }
  }
}
